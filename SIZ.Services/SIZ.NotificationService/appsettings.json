{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD};SSL Mode=${DB_SSL};Trust Server Certificate=true;"
  },
  "RabbitMQ": {
    "Host": "${RABBITMQ_HOST}",
    "Username": "${RABBITMQ_USER}",
    "Password": "${RABBITMQ_PASSWORD}"
  },
  "Redis": {
    "Host": "${REDIS_HOST}",
    "Port": "${REDIS_PORT}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
